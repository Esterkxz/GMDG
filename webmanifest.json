{
    "id": "./",
    "name": "My own EstreUI.js sample application 1",
    "short_name": "EstreUI app 1",
    "description": "Basic application model (has application bar, left main menu, bottom root tab)",
    "icons": [
        {
            "src": "./images/app_icon_192x192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "maskable"
        },
        {
            "src": "./images/app_icon_512x512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        }
    ],
    "start_url": "./serviceLoader.html",
    "theme_color": "#212426",
    "background_color": "#F1F1FA",
    "display_override": ["minimal-ui", "standalone"],
    "display": "window-controls-overlay",
    "orientation": "natural",
    "scope": ".",
    "serviceworker": {
        "scope": "./",
        "src": "./serviceWorker.js",
        "updateViaCache": false
    }
}